<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.example.techiedany.campusadmin">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_sreclogo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_sreclogo"
        android:supportsRtl="true"
        android:theme="@style/AppTheme.NoActionBar">

        <activity android:name=".dummy.LoginActivity"/>
        <activity android:name=".dummy.SignupActivity"/>
        <activity android:name=".dummy.SplashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>




        </activity>



        <activity android:name=".dummy.ResetPasswordActivity"/>

        <activity android:name=".dummy.Post">
            <intent-filter>
                <action android:name="android.intent.action.SEND" />
                <category android:name="android.intent.category.DEFAULT" />
                <data android:mimeType="image/*" />
            </intent-filter>
            <intent-filter>
                <action android:name="android.intent.action.SEND" />
                <category android:name="android.intent.category.DEFAULT" />
                <data android:mimeType="text/plain" />
            </intent-filter>

        </activity>
        <activity android:name=".dummy.ShowStudentDetailsActivity"/>

    </application>

</manifest>